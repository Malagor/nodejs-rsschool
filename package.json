{
  "name": "rs-rest-service-starter",
  "version": "1.0.0",
  "repository": {
    "type": "git",
    "url": ""
  },
  "description": "REST service starter for RS School",
  "main": "src/main.ts",
  "scripts": {
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint --ext .js,.ts --fix src/",
    "test": "cross-env DEBUG=rs:* jest --testMatch \"<rootDir>/test/e2e/test/*.test.js\" --noStackTrace --runInBand",
    "test:auth": "cross-env DEBUG=rs:* TEST_MODE=auth jest --noStackTrace",
    "doc": "jsdoc -r ./src",
    "docker:up": "docker-compose up --build",
    "docker:down": "docker-compose down",
    "typeorm": "ts-node --files -r tsconfig-paths/register ./node_modules/typeorm/cli.js --config src/common/ormconfig.ts",
    "db:create": "npm run typeorm migration:generate -n TodoMigrations",
    "db:run": "npm run typeorm -- migration:run",
    "migrationg": "ts-node ./node_modules/typeorm/cli.js --config src/common/ormconfig.ts migration:generate -n N"
  },
  "keywords": [
    "rs",
    "school",
    "rest",
    "node",
    "express",
    "autotest",
    "starter",
    "docker"
  ],
  "license": "ISC",
  "engines": {
    "node": ">=14.0.0 <15"
  },
  "jest": {
    "testEnvironment": "node",
    "setupFilesAfterEnv": [
      "./test/setup.js"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "src/**/*.ts": [
      "npm run lint"
    ]
  },
  "dependencies": {
    "@nestjs/common": "^7.6.18",
    "@nestjs/config": "^1.0.0",
    "@nestjs/core": "^7.6.18",
    "@nestjs/jwt": "^7.2.0",
    "@nestjs/passport": "^7.1.6",
    "@nestjs/platform-express": "^7.6.18",
    "@nestjs/platform-fastify": "^7.6.18",
    "@nestjs/swagger": "^5.0.0",
    "@nestjs/typeorm": "^7.1.5",
    "@types/bcrypt": "^5.0.0",
    "@types/jsonwebtoken": "^8.5.2",
    "@types/uuid": "^8.3.0",
    "bcrypt": "^5.0.1",
    "chalk": "^4.1.1",
    "class-transformer": "^0.4.0",
    "class-validator": "^0.13.1",
    "cross-env": "7.0.3",
    "dotenv": "9.0.0",
    "express": "4.17.1",
    "express-async-handler": "^1.1.4",
    "fastify-swagger": "^4.8.2",
    "http-status-codes": "^2.1.4",
    "jsonwebtoken": "^8.5.1",
    "passport-jwt": "^4.0.0",
    "passport-local": "^1.0.0",
    "pg": "^8.6.0",
    "sequelize": "^6.6.2",
    "swagger-ui-express": "^4.1.6",
    "typeorm": "^0.2.34",
    "uuid": "8.3.2",
    "yamljs": "0.3.0"
  },
  "devDependencies": {
    "@nestjs/cli": "^7.6.0",
    "@nestjs/schematics": "^7.3.1",
    "@nestjs/testing": "^7.6.18",
    "@types/express": "^4.17.12",
    "@types/node": "^15.6.1",
    "@types/passport-jwt": "^3.0.5",
    "@types/passport-local": "^1.0.33",
    "@types/swagger-ui-express": "^4.1.2",
    "@types/yamljs": "^0.2.31",
    "@typescript-eslint/eslint-plugin": "^4.25.0",
    "@typescript-eslint/parser": "^4.25.0",
    "chai": "4.3.4",
    "debug": "4.3.1",
    "dirty-chai": "2.0.1",
    "eslint": "^7.27.0",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-config-airbnb-typescript": "^12.3.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^3.4.0",
    "husky": "4.3.8",
    "jest": "26.6.3",
    "jsdoc": "^3.6.7",
    "lint-staged": "10.5.4",
    "nodemon": "^2.0.7",
    "prettier": "^2.2.1",
    "supertest": "6.1.3",
    "ts-node": "^10.0.0",
    "tsc-watch": "^4.4.0",
    "typescript": "^4.3.2"
  }
}
